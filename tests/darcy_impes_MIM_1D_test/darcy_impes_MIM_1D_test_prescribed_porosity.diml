          </constant>
        </initial_condition>
        <boundary_conditions name="Delta">
          <surface_ids>
            <integer_value shape="1" rank="1">1</integer_value>
          </surface_ids>
          <type name="dirichlet">
            <apply_weakly/>
            <python>
              <string_value lines="20" type="code" language="python">def val(X, t):
  if t&lt;1.1:
    bc=1.0e+07
  else:
    bc=0.0
  #print 'time', t
  #print 'bc', bc
  return bc</string_value>
            </python>
          </type>
        </boundary_conditions>
        <tensor_field name="Diffusivity" rank="2">
          <prescribed>
            <mesh name="ElementWiseMesh"/>
            <value name="WholeMesh">
              <isotropic>
                <constant>
                  <real_value rank="0">3.14e-07</real_value>
                </constant>
              </isotropic>
            </value>
          </prescribed>
        </tensor_field>
        <scalar_field name="Immobile" rank="0">
          <diagnostic>
            <mesh name="PressureMesh"/>
            <algorithm name="Internal"/>
            <initial_condition name="WholeMesh">
              <constant>
                <real_value rank="0">0.0</real_value>
              </constant>
            </initial_condition>
            <output/>
            <stat>
              <include_in_stat/>
            </stat>
            <consistent_interpolation/>
            <output/>
            <stat>
