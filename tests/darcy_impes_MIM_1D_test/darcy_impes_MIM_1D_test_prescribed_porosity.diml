            </stat>
          </diagnostic>
          <scalar_field name="Average_mass" rank="0">
            <diagnostic>
              <mesh name="PressureMesh"/>
              <algorithm name="Internal"/>
              <output/>
              <stat>
                <include_in_stat/>
              </stat>
              <consistent_interpolation/>
              <output/>
              <stat>
                <include_in_stat/>
              </stat>
            </diagnostic>
          </scalar_field>
          <scalar_field name="Mobile_ratio" rank="0">
            <diagnostic>
              <mesh name="PressureMesh"/>
              <algorithm name="Internal"/>
              <output/>
              <stat>
                <include_in_stat/>
              </stat>
              <consistent_interpolation/>
              <output/>
