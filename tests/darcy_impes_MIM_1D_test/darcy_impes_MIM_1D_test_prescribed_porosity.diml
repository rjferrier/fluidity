        </stat>
        <consistent_interpolation/>
      </prognostic>
    </scalar_field>
    <MobileImmobileModel>
      <scalar_field name="ImmobileSaturation" rank="0">
        <prescribed>
          <mesh name="PressureMesh"/>
          <value name="WholeMesh">
            <constant>
              <real_value rank="0">0.0035</real_value>
            </constant>
          </value>
          <output/>
          <stat>
            <include_in_stat/>
