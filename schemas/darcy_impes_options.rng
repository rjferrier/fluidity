            <ref name="darcy_impes_generic_scalar_field_bc"/>
          </zeroOrMore>
          <optional>
            <ref name="darcy_impes_generic_scalar_field_diff"/>
          </optional>
          <optional>
            <ref name="darcy_impes_generic_scalar_field_abs"/>
          </optional>
          <optional>
            <ref name="darcy_impes_generic_scalar_field_src"/>
          </optional>
          <optional>
            <ref name="darcy_impes_generic_scalar_field_src_grad"/>
          </optional>
	  <optional>
	    <element name="do_not_dilute">
	      <a:documentation>If this option is checked, the partial time differential on the LHS will be solved simply as d(scalar)/dt.  Otherwise, it will be solved as d(porosity*saturation*scalar)/dt.</a:documentation>
	      <empty/>
	    </element>
	  </optional>
          <optional>
            <ref name="darcy_MIM_generic_scalar_field_immobile_src"/>
          </optional>
          <optional>
            <ref name="darcy_impes_prog_field_chemical_source_term"/>
          </optional>
          <ref name="field_output_options"/>
          <ref name="darcy_impes_include_stat"/>
          <ref name="adaptivity_options_prognostic_scalar_field"/>
